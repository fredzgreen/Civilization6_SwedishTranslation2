<?xml version="1.0" encoding="utf-8"?><GameData>	<LocalizedText>		<!-- Default -->		<Replace Tag='LOC_DIPLOMACY_DEFAULT_NOTIFICATION_MESSAGE_INITIAL' Language='sv_SE'>			<Text>You have a message</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_DEFAULT_NOTIFICATION_SUMMARY_INITIAL' Language='sv_SE'>			<Text>{1_name} has sent you a message.</Text>		</Replace>		<!-- Declare Friend -->		<Replace Tag='LOC_DIPLOMACY_DECLARE_FRIEND_NOTIFICATION_MESSAGE_INITIAL' Language='sv_SE'>			<Text>Declaration of Friendship Request</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_DECLARE_FRIEND_NOTIFICATION_SUMMARY_INITIAL' Language='sv_SE'>			<Text>{1_name} has requested to declare a friendship between your civilizations.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_DECLARE_FRIEND_NOTIFICATION_MESSAGE_POSITIVE' Language='sv_SE'>			<Text>Declaration of Friendship Accepted</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_DECLARE_FRIEND_NOTIFICATION_SUMMARY_POSITIVE' Language='sv_SE'>			<Text>{1_name} has accepted your declaration of friendship.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_DECLARE_FRIEND_NOTIFICATION_MESSAGE_NEGATIVE' Language='sv_SE'>			<Text>Declaration of Friendship Rejected</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_DECLARE_FRIEND_NOTIFICATION_SUMMARY_NEGATIVE' Language='sv_SE'>			<Text>{1_name} has rejected your declaration of friendship.</Text>		</Replace>		<!-- Send Delegation -->		<Replace Tag='LOC_DIPLOMACY_SEND_DELEGATION_NOTIFICATION_MESSAGE_INITIAL' Language='sv_SE'>			<Text>Diplomatic Delegation Pending</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_SEND_DELEGATION_NOTIFICATION_SUMMARY_INITIAL' Language='sv_SE'>			<Text>{1_name} has asked to send a diplomatic delegation to your capital.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_SEND_DELEGATION_NOTIFICATION_MESSAGE_POSITIVE' Language='sv_SE'>			<Text>Diplomatic Delegation Accepted</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_SEND_DELEGATION_NOTIFICATION_SUMMARY_POSITIVE' Language='sv_SE'>			<Text>{1_name} has accepted your delegation.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_SEND_DELEGATION_NOTIFICATION_MESSAGE_NEGATIVE' Language='sv_SE'>			<Text>Diplomatic Delegation Rejected</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_SEND_DELEGATION_NOTIFICATION_SUMMARY_NEGATIVE' Language='sv_SE'>			<Text>{1_name} has rejected your delegation.</Text>		</Replace>		<!-- Declare War -->		<Replace Tag='LOC_DIPLOMACY_DECLARE_WAR_NOTIFICATION_MESSAGE_INITIAL' Language='sv_SE'>			<Text>War Declared</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_DECLARE_WAR_NOTIFICATION_SUMMARY_INITIAL' Language='sv_SE'>			<Text>{1_name} has declared war on your civilization.</Text>		</Replace>		<!-- Denounce -->		<Replace Tag='LOC_DIPLOMACY_DENOUNCE_NOTIFICATION_MESSAGE_INITIAL' Language='sv_SE'>			<Text>You have been Denounced</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_DENOUNCE_NOTIFICATION_SUMMARY_INITIAL' Language='sv_SE'>			<Text>{1_name} has denounced your civilization.</Text>		</Replace>		<!-- Embassy -->		<Replace Tag='LOC_DIPLOMACY_EMBASSY_NOTIFICATION_MESSAGE_INITIAL' Language='sv_SE'>			<Text>Embassy Requested</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_EMBASSY_NOTIFICATION_SUMMARY_INITIAL' Language='sv_SE'>			<Text>{1_name} wants to set up an embassy in your capital.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_EMBASSY_NOTIFICATION_MESSAGE_POSITIVE' Language='sv_SE'>			<Text>Embassy Accepted</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_EMBASSY_NOTIFICATION_SUMMARY_POSITIVE' Language='sv_SE'>			<Text>{1_name} has accepted your embassy request.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_EMBASSY_NOTIFICATION_MESSAGE_NEGATIVE' Language='sv_SE'>			<Text>Embassy Rejected</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_EMBASSY_NOTIFICATION_SUMMARY_NEGATIVE' Language='sv_SE'>			<Text>{1_name} has rejected your embassy request.</Text>		</Replace>		<!-- Make Alliance -->		<Replace Tag='LOC_DIPLOMACY_MAKE_ALLIANCE_NOTIFICATION_MESSAGE_INITIAL' Language='sv_SE'>			<Text>Alliance Requested</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_ALLIANCE_NOTIFICATION_SUMMARY_INITIAL' Language='sv_SE'>			<Text>{1_name} wants to declare an alliance between your nations.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_ALLIANCE_NOTIFICATION_MESSAGE_POSITIVE' Language='sv_SE'>			<Text>Alliance Accepted</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_ALLIANCE_NOTIFICATION_SUMMARY_POSITIVE' Language='sv_SE'>			<Text>{1_name} has accepted your alliance request.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_ALLIANCE_NOTIFICATION_MESSAGE_NEGATIVE' Language='sv_SE'>			<Text>Alliance Rejected</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_ALLIANCE_NOTIFICATION_SUMMARY_NEGATIVE' Language='sv_SE'>			<Text>{1_name} has rejected your alliance request.</Text>		</Replace>		<!-- Make Peace -->		<Replace Tag='LOC_DIPLOMACY_MAKE_PEACE_NOTIFICATION_MESSAGE_INITIAL' Language='sv_SE'>			<Text>Request for Peace</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_PEACE_NOTIFICATION_SUMMARY_INITIAL' Language='sv_SE'>			<Text>{1_name} wants to make peace with your civilization.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_PEACE_NOTIFICATION_MESSAGE_POSITIVE' Language='sv_SE'>			<Text>Peace Accepted</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_PEACE_NOTIFICATION_SUMMARY_POSITIVE' Language='sv_SE'>			<Text>{1_name} has accepted your request for peace.</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_PEACE_NOTIFICATION_MESSAGE_NEGATIVE' Language='sv_SE'>			<Text>War Continues</Text>		</Replace>		<Replace Tag='LOC_DIPLOMACY_MAKE_PEACE_NOTIFICATION_SUMMARY_NEGATIVE' Language='sv_SE'>			<Text>{1_name} has rejected your request for peace.</Text>		</Replace>		<!-- Open Borders -->		<Replace Tag='LOC_DIPLOMACY_OPEN_BORDERS_NOTIFICATION_MESSAGE_INITIAL' Language='sv_SE'>			<Text>Open Borders Request